{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:jsdom": "vitest --environment=jsdom",
    "prepare": "node -e \"if(require('fs').existsSync('.git')){require('husky').install();}\"",
    "deploy:aws": "powershell -ExecutionPolicy Bypass -File scripts/deploy-to-aws.ps1 -BucketName $env:S3_BUCKET -DistributionId $env:CLOUDFRONT_DISTRIBUTION_ID",
    "deploy:aws:linux": "bash scripts/deploy-to-aws.sh -b $S3_BUCKET ${CLOUDFRONT_DISTRIBUTION_ID:+-d $CLOUDFRONT_DISTRIBUTION_ID}"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.12",
    "@tanstack/react-virtual": "^3.13.12",
    "i18next": "^25.4.0",
    "lucide-react": "^0.544.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.7.0",
    "react-router-dom": "^7.8.1",
    "react-toastify": "^10.0.0",
    "react-window": "^1.8.11",
    "recharts": "^3.1.2",
    "sharp": "^0.34.3",
    "vite-plugin-ssr": "^0.4.142"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/oxide": "^4.1.12",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^10.3.10",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "lightningcss": "^1.30.1",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-imagetools": "^8.0.0",
    "vite-plugin-prerender": "^1.0.8",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^3.2.4",
    "zod": "^4.1.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier --check"
    ],
    "*.{json,css,md}": [
      "prettier --check"
    ]
  },
  "overrides": {
    "glob": "^10.3.10"
  }
}
